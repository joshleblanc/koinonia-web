<p style="color: green"><%= notice %></p>
<section class="section">
  <div class="container">
    <%= render "shared/hero", title: @game.name, link: games_path, link_text: "Back to games" %>
    <div class="box">
      <%= render @game %>
    </div>
    <%= turbo_frame_tag dom_id(@game, :versions), src: game_game_versions_path(@game) do %>
      <p>Loading versions...</p>
    <% end %>
  </div>
</section>
