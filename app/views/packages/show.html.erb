<p style="color: green"><%= notice %></p>
<section class="section">
  <div class="container">
    <%= render "shared/hero", title: @package.name, link: packages_path, link_text: "Back to packages" %>

    <div class="box">
      <%= render @package %>
    </div>
    <%= turbo_frame_tag dom_id(@package, :versions), src: package_package_versions_path(@package) do %>
      <p>Loading versions...</p>
    <% end %>
  </div>
</section>
