<section class="section">
  <div class="container">
    <%= turbo_frame_tag dom_id(@game, :versions) do %>
      <% @game_versions.each do |game_version| %>
        <%= render "shared/hero", title: "Versions", link: new_game_game_version_path(@game) %>
        <p style="color: green"><%= notice %></p>
        <div class="box">
          <%= render game_version %>
          <p>
            <%= link_to "Show this game version", game_version %>
          </p>
        </div>
      <% end %>
    <% end %>
  </div>
</section>
