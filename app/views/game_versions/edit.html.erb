<%= turbo_frame_tag dom_id(@game_version.game, :versions) do %>
  <%= render "shared/hero", title: "Editing game version" %>
  <section class="section">
    <div class="container">
      <%= render "form", game_version: @game_version %>
      <br>
      <div>
        <%= link_to "Show this game version", @game_version %> |
        <%= link_to "Back to game versions", game_game_versions_path(@game_version.game) %>
      </div>
    </div>
  </section>
<% end %>
